<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_main"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.main.MainFragment" />


    <fragment
        android:id="@+id/photoAlbumFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.view.photoalbum.PhotoAlbumFragment">
        <argument
            android:name="zoomViewType"
            app:argType="string"
            app:nullable="false" />
    </fragment>
    <action
        android:id="@+id/action_global_photoAlbumFragment"
        app:destination="@id/photoAlbumFragment"
        app:enterAnim="@anim/nav_enter"
        app:exitAnim="@anim/nav_exit"
        app:popEnterAnim="@anim/nav_pop_enter"
        app:popExitAnim="@anim/nav_pop_exit" />

    <fragment
        android:id="@+id/photoPagerFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.view.photoalbum.PhotoPagerFragment">
        <argument
            android:name="zoomViewType"
            app:argType="string"
            app:nullable="false" />
        <argument
            android:name="imageUris"
            app:argType="string"
            app:nullable="false" />
        <argument
            android:name="position"
            app:argType="integer"
            app:nullable="false" />
    </fragment>
    <action
        android:id="@+id/action_global_photoPagerFragment"
        app:destination="@id/photoPagerFragment"
        app:enterAnim="@anim/nav_enter"
        app:exitAnim="@anim/nav_exit"
        app:popEnterAnim="@anim/nav_pop_enter"
        app:popExitAnim="@anim/nav_pop_exit" />


    <fragment
        android:id="@+id/myZoomImagePagerFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.compose.zoomimage.MyZoomImagePagerFragment" />
    <action
        android:id="@+id/action_global_myZoomImagePagerFragment"
        app:destination="@id/myZoomImagePagerFragment"
        app:enterAnim="@anim/nav_enter"
        app:exitAnim="@anim/nav_exit"
        app:popEnterAnim="@anim/nav_pop_enter"
        app:popExitAnim="@anim/nav_pop_exit" />


    <fragment
        android:id="@+id/telephotoZoomableAsyncImagePagerFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.compose.telephoto.TelephotoZoomableAsyncImagePagerFragment" />
    <action
        android:id="@+id/action_global_telephotoZoomableAsyncImagePagerFragment"
        app:destination="@id/telephotoZoomableAsyncImagePagerFragment"
        app:enterAnim="@anim/nav_enter"
        app:exitAnim="@anim/nav_exit"
        app:popEnterAnim="@anim/nav_pop_enter"
        app:popExitAnim="@anim/nav_pop_exit" />


    <fragment
        android:id="@+id/zoomImageViewFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.view.zoomimage.ZoomImageViewFragment">
        <argument
            android:name="imageUri"
            app:argType="string"
            app:nullable="false" />
    </fragment>

    <fragment
        android:id="@+id/zoomImageViewPagerFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.view.zoomimage.ZoomImageViewPagerFragment" />
    <action
        android:id="@+id/action_global_zoomImageViewPagerFragment"
        app:destination="@id/zoomImageViewPagerFragment"
        app:enterAnim="@anim/nav_enter"
        app:exitAnim="@anim/nav_exit"
        app:popEnterAnim="@anim/nav_pop_enter"
        app:popExitAnim="@anim/nav_pop_exit" />


    <fragment
        android:id="@+id/sketchZoomImageViewFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.view.zoomimage.SketchZoomImageViewFragment">
        <argument
            android:name="imageUri"
            app:argType="string"
            app:nullable="false" />
    </fragment>

    <fragment
        android:id="@+id/sketchZoomImageViewPagerFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.view.zoomimage.SketchZoomImageViewPagerFragment" />
    <action
        android:id="@+id/action_global_sketchZoomImageViewPagerFragment"
        app:destination="@id/sketchZoomImageViewPagerFragment"
        app:enterAnim="@anim/nav_enter"
        app:exitAnim="@anim/nav_exit"
        app:popEnterAnim="@anim/nav_pop_enter"
        app:popExitAnim="@anim/nav_pop_exit" />


    <fragment
        android:id="@+id/coilZoomImageViewFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.view.zoomimage.CoilZoomImageViewFragment">
        <argument
            android:name="imageUri"
            app:argType="string"
            app:nullable="false" />
    </fragment>

    <fragment
        android:id="@+id/coilZoomImageViewPagerFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.view.zoomimage.CoilZoomImageViewPagerFragment" />
    <action
        android:id="@+id/action_global_coilZoomImageViewPagerFragment"
        app:destination="@id/coilZoomImageViewPagerFragment"
        app:enterAnim="@anim/nav_enter"
        app:exitAnim="@anim/nav_exit"
        app:popEnterAnim="@anim/nav_pop_enter"
        app:popExitAnim="@anim/nav_pop_exit" />


    <fragment
        android:id="@+id/glideZoomImageViewFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.view.zoomimage.GlideZoomImageViewFragment">
        <argument
            android:name="imageUri"
            app:argType="string"
            app:nullable="false" />
    </fragment>

    <fragment
        android:id="@+id/glideZoomImageViewPagerFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.view.zoomimage.GlideZoomImageViewPagerFragment" />
    <action
        android:id="@+id/action_global_glideZoomImageViewPagerFragment"
        app:destination="@id/glideZoomImageViewPagerFragment"
        app:enterAnim="@anim/nav_enter"
        app:exitAnim="@anim/nav_exit"
        app:popEnterAnim="@anim/nav_pop_enter"
        app:popExitAnim="@anim/nav_pop_exit" />


    <fragment
        android:id="@+id/picassoZoomImageViewFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.view.zoomimage.PicassoZoomImageViewFragment">
        <argument
            android:name="imageUri"
            app:argType="string"
            app:nullable="false" />
    </fragment>

    <fragment
        android:id="@+id/picassoZoomImageViewPagerFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.view.zoomimage.PicassoZoomImageViewPagerFragment" />
    <action
        android:id="@+id/action_global_picassoZoomImageViewPagerFragment"
        app:destination="@id/picassoZoomImageViewPagerFragment"
        app:enterAnim="@anim/nav_enter"
        app:exitAnim="@anim/nav_exit"
        app:popEnterAnim="@anim/nav_pop_enter"
        app:popExitAnim="@anim/nav_pop_exit" />

    <fragment
        android:id="@+id/photoViewPagerFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.view.photoview.PhotoViewPagerFragment" />
    <action
        android:id="@+id/action_global_photoViewPagerFragment"
        app:destination="@id/photoViewPagerFragment"
        app:enterAnim="@anim/nav_enter"
        app:exitAnim="@anim/nav_exit"
        app:popEnterAnim="@anim/nav_pop_enter"
        app:popExitAnim="@anim/nav_pop_exit" />

    <fragment
        android:id="@+id/photoViewFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.view.photoview.PhotoViewFragment">
        <argument
            android:name="imageUri"
            app:argType="string"
            app:nullable="false" />
    </fragment>

    <fragment
        android:id="@+id/subsamplingViewPagerFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.view.subsamplingview.SubsamplingViewPagerFragment" />
    <action
        android:id="@+id/action_global_subsamplingViewPagerFragment"
        app:destination="@id/subsamplingViewPagerFragment"
        app:enterAnim="@anim/nav_enter"
        app:exitAnim="@anim/nav_exit"
        app:popEnterAnim="@anim/nav_pop_enter"
        app:popExitAnim="@anim/nav_pop_exit" />

    <fragment
        android:id="@+id/subsamplingViewFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.view.subsamplingview.SubsamplingViewFragment">
        <argument
            android:name="imageUri"
            app:argType="string"
            app:nullable="false" />
    </fragment>

    <fragment
        android:id="@+id/layoutOrientationTestFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.view.test.LayoutOrientationTestFragment" />
    <action
        android:id="@+id/action_global_layoutOrientationTestFragment"
        app:destination="@id/layoutOrientationTestFragment"
        app:enterAnim="@anim/nav_enter"
