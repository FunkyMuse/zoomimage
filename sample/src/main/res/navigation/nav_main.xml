<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_main"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.MainFragment" />


    <fragment
        android:id="@+id/myZoomImageFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.MyZoomImageFragment" />
    <action
        android:id="@+id/action_global_myZoomImageFragment"
        app:destination="@id/myZoomImageFragment"
        app:enterAnim="@anim/nav_enter"
        app:exitAnim="@anim/nav_exit"
        app:popEnterAnim="@anim/nav_pop_enter"
        app:popExitAnim="@anim/nav_pop_exit" />

    <fragment
        android:id="@+id/zoomImageViewPagerFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.myview.ZoomImageViewPagerFragment" />
    <action
        android:id="@+id/action_global_zoomImageViewPagerFragment"
        app:destination="@id/zoomImageViewPagerFragment"
        app:enterAnim="@anim/nav_enter"
        app:exitAnim="@anim/nav_exit"
        app:popEnterAnim="@anim/nav_pop_enter"
        app:popExitAnim="@anim/nav_pop_exit" />

    <fragment
        android:id="@+id/sketchZoomImageViewPagerFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.myview.SketchZoomImageViewPagerFragment" />
    <action
        android:id="@+id/action_global_sketchZoomImageViewPagerFragment"
        app:destination="@id/sketchZoomImageViewPagerFragment"
        app:enterAnim="@anim/nav_enter"
        app:exitAnim="@anim/nav_exit"
        app:popEnterAnim="@anim/nav_pop_enter"
        app:popExitAnim="@anim/nav_pop_exit" />

    <fragment
        android:id="@+id/telephotoZoomImageFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.TelephotoZoomImageFragment" />
    <action
        android:id="@+id/action_global_telephotoZoomImageFragment"
        app:destination="@id/telephotoZoomImageFragment"
        app:enterAnim="@anim/nav_enter"
        app:exitAnim="@anim/nav_exit"
        app:popEnterAnim="@anim/nav_pop_enter"
        app:popExitAnim="@anim/nav_pop_exit" />

    <!--<fragment
        android:id="@+id/myZoomImageViewFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.myview.MyZoomImageViewFragment" />
    <action
        android:id="@+id/action_global_myZoomImageViewFragment"
        app:destination="@id/myZoomImageViewFragment"
        app:enterAnim="@anim/nav_enter"
        app:exitAnim="@anim/nav_exit"
        app:popEnterAnim="@anim/nav_pop_enter"
        app:popExitAnim="@anim/nav_pop_exit" />-->


    <fragment
        android:id="@+id/zoomImageViewFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.myview.ZoomImageViewFragment">
        <argument
            android:name="imageUri"
            app:argType="string"
            app:nullable="false" />
    </fragment>
    <fragment
        android:id="@+id/sketchZoomImageViewFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.myview.SketchZoomImageViewFragment">
        <argument
            android:name="imageUri"
            app:argType="string"
            app:nullable="false" />
    </fragment>



    <dialog
        android:id="@+id/imageInfoDialogFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.myview.ImageInfoDialogFragment">
        <argument
            android:name="uri"
            android:defaultValue="null"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="optionsInfo"
            android:defaultValue="null"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="imageInfo"
            android:defaultValue="null"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="bitmapInfo"
            android:defaultValue="null"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="drawableInfo"
            android:defaultValue="null"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="dataFromInfo"
            android:defaultValue="null"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="transformedInfo"
            android:defaultValue="null"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="zoomInfo"
            android:defaultValue="null"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="tilesInfo"
            android:defaultValue="null"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="throwableString"
            android:defaultValue="null"
            app:argType="string"
            app:nullable="true" />
    </dialog>
    <action
        android:id="@+id/action_global_imageInfoDialogFragment"
        app:destination="@id/imageInfoDialogFragment" />

    <fragment
        android:id="@+id/photoViewPagerFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.photoview.PhotoViewPagerFragment" />
    <action
        android:id="@+id/action_global_photoViewPagerFragment"
        app:destination="@id/photoViewPagerFragment"
        app:enterAnim="@anim/nav_enter"
        app:exitAnim="@anim/nav_exit"
        app:popEnterAnim="@anim/nav_pop_enter"
        app:popExitAnim="@anim/nav_pop_exit" />

    <fragment
        android:id="@+id/photoViewFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.photoview.PhotoViewFragment">
        <argument
            android:name="imageUri"
            app:argType="string"
            app:nullable="false" />
    </fragment>

    <fragment
        android:id="@+id/subsamplingViewPagerFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.subsamplingview.SubsamplingViewPagerFragment" />
    <action
        android:id="@+id/action_global_subsamplingViewPagerFragment"
        app:destination="@id/subsamplingViewPagerFragment"
        app:enterAnim="@anim/nav_enter"
        app:exitAnim="@anim/nav_exit"
        app:popEnterAnim="@anim/nav_pop_enter"
        app:popExitAnim="@anim/nav_pop_exit" />

    <fragment
        android:id="@+id/subsamplingViewFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.subsamplingview.SubsamplingViewFragment">
        <argument
            android:name="imageUri"
            app:argType="string"
            app:nullable="false" />
    </fragment>

    <fragment
        android:id="@+id/orientationTestFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.myview.OrientationTestFragment" />
    <action
        android:id="@+id/action_global_orientationTestFragment"
        app:destination="@id/orientationTestFragment"
        app:enterAnim="@anim/nav_enter"
        app:exitAnim="@anim/nav_exit"
        app:popEnterAnim="@anim/nav_pop_enter"
        app:popExitAnim="@anim/nav_pop_exit" />
</navigation>