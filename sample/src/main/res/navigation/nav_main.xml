<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_main"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.MainFragment" />


    <fragment
        android:id="@+id/myZoomImageFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.MyZoomImageFragment" />
    <action
        android:id="@+id/action_global_myZoomImageFragment"
        app:destination="@id/myZoomImageFragment"
        app:enterAnim="@anim/nav_enter"
        app:exitAnim="@anim/nav_exit"
        app:popEnterAnim="@anim/nav_pop_enter"
        app:popExitAnim="@anim/nav_pop_exit" />


    <fragment
        android:id="@+id/zoomImageViewFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.zoomimage.ZoomImageViewFragment">
        <argument
            android:name="imageUri"
            app:argType="string"
            app:nullable="false" />
    </fragment>

    <fragment
        android:id="@+id/zoomImageViewPagerFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.zoomimage.ZoomImageViewPagerFragment" />
    <action
        android:id="@+id/action_global_zoomImageViewPagerFragment"
        app:destination="@id/zoomImageViewPagerFragment"
        app:enterAnim="@anim/nav_enter"
        app:exitAnim="@anim/nav_exit"
        app:popEnterAnim="@anim/nav_pop_enter"
        app:popExitAnim="@anim/nav_pop_exit" />


    <fragment
        android:id="@+id/sketchZoomImageViewFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.sketch.SketchZoomImageViewFragment">
        <argument
            android:name="imageUri"
            app:argType="string"
            app:nullable="false" />
    </fragment>

    <fragment
        android:id="@+id/sketchZoomImageViewPagerFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.sketch.SketchZoomImageViewPagerFragment" />
    <action
        android:id="@+id/action_global_sketchZoomImageViewPagerFragment"
        app:destination="@id/sketchZoomImageViewPagerFragment"
        app:enterAnim="@anim/nav_enter"
        app:exitAnim="@anim/nav_exit"
        app:popEnterAnim="@anim/nav_pop_enter"
        app:popExitAnim="@anim/nav_pop_exit" />


    <fragment
        android:id="@+id/coilZoomImageViewFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.coil.CoilZoomImageViewFragment">
        <argument
            android:name="imageUri"
            app:argType="string"
            app:nullable="false" />
    </fragment>

    <fragment
        android:id="@+id/coilZoomImageViewPagerFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.coil.CoilZoomImageViewPagerFragment" />
    <action
        android:id="@+id/action_global_coilZoomImageViewPagerFragment"
        app:destination="@id/coilZoomImageViewPagerFragment"
        app:enterAnim="@anim/nav_enter"
        app:exitAnim="@anim/nav_exit"
        app:popEnterAnim="@anim/nav_pop_enter"
        app:popExitAnim="@anim/nav_pop_exit" />


    <fragment
        android:id="@+id/glideZoomImageViewFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.glide.GlideZoomImageViewFragment">
        <argument
            android:name="imageUri"
            app:argType="string"
            app:nullable="false" />
    </fragment>

    <fragment
        android:id="@+id/glideZoomImageViewPagerFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.glide.GlideZoomImageViewPagerFragment" />
    <action
        android:id="@+id/action_global_glideZoomImageViewPagerFragment"
        app:destination="@id/glideZoomImageViewPagerFragment"
        app:enterAnim="@anim/nav_enter"
        app:exitAnim="@anim/nav_exit"
        app:popEnterAnim="@anim/nav_pop_enter"
        app:popExitAnim="@anim/nav_pop_exit" />


    <fragment
        android:id="@+id/telephotoZoomImageFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.TelephotoZoomImageFragment" />
    <action
        android:id="@+id/action_global_telephotoZoomImageFragment"
        app:destination="@id/telephotoZoomImageFragment"
        app:enterAnim="@anim/nav_enter"
        app:exitAnim="@anim/nav_exit"
        app:popEnterAnim="@anim/nav_pop_enter"
        app:popExitAnim="@anim/nav_pop_exit" />

    <fragment
        android:id="@+id/photoViewPagerFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.photoview.PhotoViewPagerFragment" />
    <action
        android:id="@+id/action_global_photoViewPagerFragment"
        app:destination="@id/photoViewPagerFragment"
        app:enterAnim="@anim/nav_enter"
        app:exitAnim="@anim/nav_exit"
        app:popEnterAnim="@anim/nav_pop_enter"
        app:popExitAnim="@anim/nav_pop_exit" />

    <fragment
        android:id="@+id/photoViewFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.photoview.PhotoViewFragment">
        <argument
            android:name="imageUri"
            app:argType="string"
            app:nullable="false" />
    </fragment>

    <fragment
        android:id="@+id/subsamplingViewPagerFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.subsamplingview.SubsamplingViewPagerFragment" />
    <action
        android:id="@+id/action_global_subsamplingViewPagerFragment"
        app:destination="@id/subsamplingViewPagerFragment"
        app:enterAnim="@anim/nav_enter"
        app:exitAnim="@anim/nav_exit"
        app:popEnterAnim="@anim/nav_pop_enter"
        app:popExitAnim="@anim/nav_pop_exit" />

    <fragment
        android:id="@+id/subsamplingViewFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.subsamplingview.SubsamplingViewFragment">
        <argument
            android:name="imageUri"
            app:argType="string"
            app:nullable="false" />
    </fragment>

    <fragment
        android:id="@+id/orientationTestFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.test.OrientationTestFragment" />
    <action
        android:id="@+id/action_global_orientationTestFragment"
        app:destination="@id/orientationTestFragment"
        app:enterAnim="@anim/nav_enter"
        app:exitAnim="@anim/nav_exit"
        app:popEnterAnim="@anim/nav_pop_enter"
        app:popExitAnim="@anim/nav_pop_exit" />
</navigation>