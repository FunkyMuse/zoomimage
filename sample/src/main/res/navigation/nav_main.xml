    <fragment
        android:id="@+id/exifOrientationTestFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.view.test.ExifOrientationTestFragment" />
    <action
        android:id="@+id/action_global_exifOrientationTestFragment"
        app:destination="@id/exifOrientationTestFragment"
        app:enterAnim="@anim/nav_enter"
        app:exitAnim="@anim/nav_exit"
        app:popEnterAnim="@anim/nav_pop_enter"
        app:popExitAnim="@anim/nav_pop_exit" />

    <fragment
        android:id="@+id/zoomImageViewSettingsDialogFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.view.zoomimage.ZoomImageViewSettingsDialogFragment">
        <argument
            android:name="supportMemoryCache"
            app:argType="boolean"
            app:nullable="false" />
        <argument
            android:name="supportIgnoreExifOrientation"
            app:argType="boolean"
            app:nullable="false" />
        <argument
            android:name="supportReuseBitmap"
            app:argType="boolean"
            app:nullable="false" />
    </fragment>

    <fragment
        android:id="@+id/zoomImageViewInfoDialogFragment"
        android:name="com.github.panpf.zoomimage.sample.ui.view.zoomimage.ZoomImageViewInfoDialogFragment">
        <argument
            android:name="imageUri"
            app:argType="string"
            app:nullable="false" />
        <argument
            android:name="imageInfo"
            app:argType="string"
            app:nullable="false" />
        <argument
            android:name="sizeInfo"
            app:argType="string"
            app:nullable="false" />
        <argument
            android:name="tilesInfo"
            app:argType="string"
            app:nullable="false" />
    </fragment>
</navigation>